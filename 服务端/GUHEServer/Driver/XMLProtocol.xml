  <!--命令：PushPanelAlarm 上报报警主机报警防区-->
  <!--参数：1：主机地址-->
  <!--数据：2: 报警防区编号-->
  <!--数据：3: 报警防区状态 0 正常待警  1 报警  2 防区旁路 3 防区故障>-->
  <!--特殊防区编码\地址
  ("LowBatty",  300);
  ("AC Loss",   301);
  ("EnterProg", 302);
  ("PanelArm" , 303);
  ("PanelDsp",  304);
  ("PassWord",  305);
  -->
<!--  取消
  <GHISMS>
    <command>
      <code>1</code>
      <function>PushPanelAlarm</function>
      <hostaddr/>
      <zone/>
      <zonestatus/>
    </command>
  </GHISMS>-->
  <!--命令：PushPanelInfo 上报报警主机显示信息-->
  <!--参数：1：主机地址-->
  <!--参数：2：主机状态-->
  <!--主机状态对应的枚举类型
  public enum VistaPanelState
  {
      VistaTrouble = 2,
      VistaReady = 16,
      VistaFire = 32,
      VistaLowBattery = 64,
      VistaArmStay = 128,
      VistaAlarm = 256,
      VistaArmAway = 512,
      VistaACPower = 1024,
      VistaBypass = 2048,
      VistaChime = 4096,
      VistaProgramBit = 819 2,
      VistaArmInstant = 16384,
      }


  <GHISMS>
    <command>
      <code>2</code>
      <function>PushPanelInfo</function>
      <hostaddr/>
      <dispinfo/>
      <panelstatus/>
      <zone/>
      <isplayback/>
    </command>
  </GHISMS>
    -->
  <!--命令：SetPanelStatus 主机操作-->
  <!--参数：1：主机地址-->
  <!--参数：2：主机状态-->
  <!--panelstatus
  外出布防	密码 + AWAY [2].
  留守布防	密码 + STAY [3].
  即时布防	密码 + INSTANT [7].
  最大布防	密码 + MAXIMUM [4].
  撤防	密码 + OFF [1].
  -->
  

  <GHISMS>
    <command>
      <code>3</code>
      <function>SetPanelStatus</function>
      <hostname/>
      <part/>
      <panelstatus/>
    </command>
  </GHISMS>


  <!--命令：SetPanelZoneBypass 主机操作-->
  <!--参数：1：主机地址-->
  <!--参数：2：防区号-->
  <!--panelstatus
  防区旁路	密码 + BYPASS [6] + 防区号码
  -->

  <GHISMS>
    <command>
      <code>4</code>
      <function>SetAlarmZoneBypass</function>
      <hostaddr/>
      <zone/>
    </command>
  </GHISMS>
  
